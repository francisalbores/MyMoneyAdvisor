<h1>Enter Expenses</h1>
<div class="form-group">
	<label>Name</label>
	<input type="text" ng-model="expense.name" class="form-control" />
</div>
<div class="form-group">
	<label>Php</label>
	<input type="number" ng-model="expense.cost" class="form-control" />
</div>
<input class="btn btn-primary" type="submit" ng-click="addExpense()" value="Save" />